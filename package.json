{
  "name": "@once-ui-system/magic-portfolio",
  "version": "1.3.0",
  "scripts": {
    "dev": "next dev",
    "export": "next export",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "npm run clean:build && npm run clean:cache && npm run clean:modules",
    "clean:build": "rm -rf .next out build dist",
    "clean:cache": "rm -rf .cache .parcel-cache .turbo .swc .eslintcache && rm -f tsconfig.tsbuildinfo",
    "clean:modules": "rm -rf node_modules package-lock.json",
    "clean:all": "npm run clean && rm -rf .DS_Store && rm -f *.log",
    "reinstall": "npm run clean:modules && npm install",
    "fresh": "npm run clean:all && npm install",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.1",
    "@mdx-js/loader": "^3.1.0",
    "@next/mdx": "^14.2.16",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "cookie": "^1.0.1",
    "dotenv": "^17.2.1",
    "gray-matter": "^4.0.3",
    "mongodb": "^6.20.0",
    "next": "^14.2.16",
    "next-mdx-remote": "^5.0.0",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.5.15",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-masonry-css": "^1.0.16",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sass": "^1.77.6",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^2.1.1",
    "@types/cookie": "^0.6.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.16",
    "postcss-custom-media": "^10.0.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "typescript": "^5"
  }
}
